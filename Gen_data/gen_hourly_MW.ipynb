    "import pandas as pd\n",
    "# Load the aggregated hourly data\n",
    "df_all = pd.read_csv(\"gen_hourly_MW_all.csv\")\n",
    "\n",
    "# Calculate total generation per production type\n",
    "total_gen = df_all.groupby(\"Production Type\")[\"Generation (MW)\"].sum().sort_values(ascending=False)\n",
    "\n",
    "# Create a ranking DataFrame\n",
    "ranking = pd.DataFrame({\n",
    "    'Production Type': total_gen.index,\n",
    "    'Total Generation (MWh)': total_gen.values,\n",
    "    'Rank': range(1, len(total_gen) + 1)\n",
    "})\n",
    "\n",
    "print(\"Total Generation Ranking by Production Type:\")\n",
    "print(\"=\" * 60)\n",
    "print(ranking.to_string(index=False))\n",
    "print(\"\\n\" + \"=\" * 60)\n",
    "print(f\"Total Generation All Types: {total_gen.sum():,.2f} MWh\")"
